{% extends 'LiganetCoreBundle::base.html.twig' %}
{% block content %}
<h1>SpielArt</h1>

<table class="table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Namekurz</th>
            <td>{{ entity.nameKurz }}</td>
        </tr>
        <tr>
            <th>Anzahlspieler</th>
            <td>{{ entity.anzahlSpieler }}</td>
        </tr>
        <tr>
            <th>Mixte</th>
            <td>{{ entity.mixte }}</td>
        </tr>
        <tr>
            <th>Nummer</th>
            <td>{{ entity.nummer }}</td>
        </tr>
        <tr>
            <th>Reihenfolge</th>
            <td>{{ entity.reihenfolge }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('spielart') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('spielart_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('spielart_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
