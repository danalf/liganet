{% extends 'LiganetCoreBundle::base.html.twig' %}
{% block content %}
<div class="page-header">
    <h1>Spieler bearbeiten: {{ entity }}</h1>
</div>

<form action="{{ path('spieler_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button class="btn" type="submit">Speichern</button>
    </p>
</form>

{% endblock %}
