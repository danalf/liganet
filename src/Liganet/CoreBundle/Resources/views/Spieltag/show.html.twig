<h1>Spieltag</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Nummer</th>
            <td>{{ entity.nummer }}</td>
        </tr>
        <tr>
            <th>Datum</th>
            <td>{{ entity.datum|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('spieltag') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('spieltag_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('spieltag_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
