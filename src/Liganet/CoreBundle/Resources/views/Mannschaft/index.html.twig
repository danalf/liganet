            <th>Rang</th>
            <th>Bemerkung</th>
            <th>Kapit√§n</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('mannschaft_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity }}</td>
            <td>{{ entity.bemerkung }}</td>
            <td>{{ entity.captain }}</td>
            <td>
