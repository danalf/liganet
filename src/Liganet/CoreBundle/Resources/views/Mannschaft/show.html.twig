{% extends '::base.html.twig' %}
{% block content %}
<h1>Mannschaft</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Rang</th>
            <td>{{ entity.rang }}</td>
        </tr>
        <tr>
            <th>Bemerkung</th>
            <td>{{ entity.bemerkung }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('mannschaft') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('mannschaft_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('mannschaft_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}