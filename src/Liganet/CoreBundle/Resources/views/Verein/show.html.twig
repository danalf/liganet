<h1>Verein</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Name</th>
            <td>{{ entity.name }}</td>
        </tr>
        <tr>
            <th>Namekurz</th>
            <td>{{ entity.namekurz }}</td>
        </tr>
        <tr>
            <th>Kuerzel</th>
            <td>{{ entity.kuerzel }}</td>
        </tr>
        <tr>
            <th>Nummer</th>
            <td>{{ entity.nummer }}</td>
        </tr>
        <tr>
            <th>Kontakt</th>
            <td>{{ entity.kontakt }}</td>
        </tr>
        <tr>
            <th>Homepage</th>
            <td>{{ entity.homepage }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('verein') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('verein_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('verein_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
