{% extends '::base.html.twig' %}
{% block content %}
<div class="page-header">
    <h1>Verein bearbeiten: {{ entity.namekurz }}</h1>
</div>

<form action="{{ path('verein_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(edit_form) }}>
    {{ form_widget(edit_form) }}
    <p>
        <button class="btn" type="submit">Speichern</button>
    </p>
</form>

{% endblock %}
