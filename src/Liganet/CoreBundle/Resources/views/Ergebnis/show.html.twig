<h1>Ergebnis</h1>

<table class="table">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Platz</th>
            <td>{{ entity.platz }}</td>
        </tr>
        <tr>
            <th>Bemerkung</th>
            <td>{{ entity.bemerkung }}</td>
        </tr>
        <tr>
            <th>Unterschrift_ligaleiter</th>
            <td>{{ entity.unterschriftligaleiter }}</td>
        </tr>
        <tr>
            <th>Kugeln1</th>
            <td>{{ entity.kugeln1 }}</td>
        </tr>
        <tr>
            <th>Kugeln2</th>
            <td>{{ entity.kugeln2 }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('ergebnis') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ergebnis_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('ergebnis_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
